<odoo>
  <data>

     <record model="ir.ui.view" id="cabinet_form_view">
            <field name="name">course.form</field>
            <field name="model">cabinet.medical</field>
            <field name="arch" type="xml">
                <form string="Cabinet Form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
                            <notebook>
                            <page string="Description">
                                <field name="description"/>
                            </page>
                            <page string="About">
                                This is an example of notebooks
                            </page>
                        </notebook>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

 <record model="ir.ui.view" id="course_search_view">
            <field name="name">course.search</field>
            <field name="model">cabinet.medical</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <field name="description"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="cabinet_list_action">
            <field name="name">Etablissement</field>
            <field name="res_model">cabinet.medical</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Créer un établissement
                </p>
            </field>
        </record>

        <!-- top level menu: no parent -->
        <menuitem id="main_cabinet_menu" name="Cabinet Medical"/>
        <!-- A first level in the left side menu is needed
             before using action= attribute -->
        <menuitem id="cabinet_menu" name="Cabinet Medical"
                  parent="main_cabinet_menu"/>
        <!-- the following menuitem should appear *after*
             its parent openacademy_menu and *after* its
             action course_list_action -->
        <menuitem id="courses_menu" name="Etablissement" parent="cabinet_menu"
                  action="cabinet_list_action"/>
        <!-- Full id location:
             action="openacademy.course_list_action"
             It is not required when it is the same module -->
  </data>
</odoo>